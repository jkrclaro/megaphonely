{
  "AWSEBDockerrunVersion": "2",
  "volumes": [
    {
      "name": "twitter",
      "host": {
        "sourcePath": "/var/app/current/twitter"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "twitter",
      "image": "jkrclaro/penguin",
      "essential": true,
      "memory": 128,
      "command": ["npm", "run", "twitter"]
    }
  ]
}
